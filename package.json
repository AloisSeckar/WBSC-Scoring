{
  "name": "wbsc-scoring",
  "version": "1.5.0",
  "private": true,
  "type": "module",
  "scripts": {
    "eslint": "eslint .",
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
